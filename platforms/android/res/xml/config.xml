<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ekurhuleni.indigentapp" version="1.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="OneSignalPush">
        <param name="android-package" value="com.plugin.gcm.OneSignalPush" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <feature name="FileOpener2">
        <param name="android-package" value="io.github.pwlin.cordova.plugins.fileopener2.FileOpener2" />
    </feature>
    <name>Indigent Field Worker App</name>
    <description>
        Indigent Field Worker App
    </description>
    <author email="tsheko.mashego@ekurhuleni.co.za" href="http://www.ekurhuleni.co.za">
        Tsheko Mashego
    </author>
    <content src="index.html" />
    <icon src="res/ekurhuleni.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <icon density="mdpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <icon density="hdpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <icon density="xhdpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <icon density="xxhdpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <icon density="xxxhdpi" src="res/icon/android/Webp.net-resizeimage.png" />
    <splash density="land-hdpi" src="res/screen/android/ekurhuleni-hdpi-landscape.png" />
    <splash density="land-ldpi" src="res/screen/android/ekurhuleni-ldpi-landscapei.png" />
    <splash density="land-mdpi" src="res/screen/android/ekurhuleni-mdpi-landscape.png" />
    <splash density="land-xhdpi" src="res/screen/android/ekurhuleni-xhdpi-landscape.png" />
    <splash density="port-hdpi" src="res/screen/android/ekurhuleni-ldpi-portrait.png" />
    <splash density="port-ldpi" src="res/screen/android/ekurhuleni-hdpi-portrait.png" />
    <splash density="port-mdpi" src="res/screen/android/ekurhuleni-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/screen/android/ekurhuleni-xhdpi-portrait.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
